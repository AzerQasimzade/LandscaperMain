@using LandscaperProject.Areas.ViewModels.Service;
@model CreateServiceVM

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div az class="col-12 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Creating Service </h4>
                        <form class="forms-sample" method="post" enctype="multipart/form-data">
                            <div asp-validation-summary="ModelOnly"></div>
                            <div class="form-group">
                                <label asp-for="Title" for="exampleInputName1">Title</label>
                                <input asp-for="Title" class="form-control" id="exampleInputName1" placeholder="Title">
                                <span asp-validation-for="Title"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Description" for="exampleInputName1">Description</label>
                                <input asp-for="Description" class="form-control" id="exampleInputName1" placeholder="Description">
                                <span asp-validation-for="Description"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Photo" for="exampleInputName1">Photo</label>
                                <input asp-for="Photo" class="form-control" id="exampleInputName1" placeholder="Title">
                                <span asp-validation-for="Photo"></span>
                            </div>
                            <button type="submit" class="btn btn-primary me-2">Submit</button>
                            <button class="btn btn-light">Cancel</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

